Class AddingTwoNum{

public static void main (String [] args)
{
int a = 10;
int b = 20;
int sum = a+b;
System.out.println(sum);
}

}
 
Class Multiple {

public static void main (String [] args)
{
int a = 10;
int b = 20;
int mult = a*b;
System.out.println(mult);

}
}
class Substraction {

  public static void main(String[] args) {
    int a = 10;
    int b = 20;
    System.out.println(a-b);
  }
}
